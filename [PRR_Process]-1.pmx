      <record>
        <tri_uid>1365370715b0842d0ec63c5067721997</tri_uid>
        <tri_title>notifyChiefRiskOfficer</tri_title>
        <tri_description></tri_description>
        <pro_uid>6702262155a6744606ca5d1007883498</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[if(@@insuranceRiskRmks == "" ){
	@@insuranceRiskRmks = @@insuranceTempRmk;
}else{
	@@insuranceRiskRmks = @@insuranceTempRmk . "\n................\n" . @@insuranceRiskRmks;
}
$caseOwner = userInfo(@@CaseOwnerID);
if(@@insuranceRiskApprovalAction == "1"){
	if(@@insuranceRiskApprovalAction == "1"){
		@@insuranceRiskApproved = "Yes";
	}else{
		@@insuranceRiskApproved = "Referred";
	}
	
	$data = userInfo(@@USER_LOGGED);
	$groupName1 = 'PRR_Chief_Risk_Officer_GroupUID';  //change to name of group
	
	$result1 = '';
	
	$to = '';

	
		$query1 = "SELECT DISTINCT GU.USR_UID FROM CONTENT AS C, GROUP_USER AS GU WHERE C.CON_CATEGORY='GRP_TITLE' AND C.CON_VALUE='$groupName1' AND C.CON_ID=GU.GRP_UID";
		$result1 = executeQuery($query1);

		foreach ($result1 as $record) {
		  $aInfo = userInfo($record['USR_UID']);
		  if (empty($to))
			 $to = $aInfo['mail'];
		  else
			 $to .= ', ' . $aInfo['mail'];
	   }
	

	

	if ($to == '') {
	   G::SendMessageText("Unable to find any users in group '$groupName1'.", "WARNING");
	}
	else {
		@@email_salute = $caseOwner['firstname'];
		@@approving_level = "the Chief Risk Officer";
		PMFSendMessage(@@APPLICATION, $data['mail'], $caseOwner['mail'], '', '',
		  'APPROVAL PROCESSMAKER Case ID:'. @@APPNO, 'NotifyNextUser.html');
		
		@@email_salute = "All";
	   PMFSendMessage(@@APPLICATION, $data['mail'], $to, '', '', 'APPROVAL PROCESSMAKER Case ID:'. @@APPNO, 'RouteFeedBack.html');
	}
}else if(@@insuranceRiskApprovalAction == "2"){
	//send an email to the requester that their request has been denied
	@@insuranceRiskApproved = "No";
	@@approved_by = @@insuranceRiskOfficer;
	$lm = userInfo(@@USER_LOGGED);
	//requestDenied.html
	@@email_salute = $caseOwner['firstname'];
	PMFSendMessage(@@APPLICATION, $lm['mail'], $caseOwner['mail'], '', '',
		  'APPROVAL PROCESSMAKER Case ID:'. @@APPNO, 'requestDenied.html');
}]]></tri_webbot>
        <tri_param></tri_param>
      </record>
      <record>
        <tri_uid>1660170215ac76090be8cc8088613698</tri_uid>
        <tri_title>mergeUnderwriterApprovals</tri_title>
        <tri_description><![CDATA[This trigger ensures that all the approvals in underwriter lane are merged. if there is any deny approval at this stage, the request is returned to the requester, otherwise it is forward to the next lane which is the head technical]]></tri_description>
        <pro_uid>6702262155a6744606ca5d1007883498</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[if(@@underwriters_approval_count >= count(@=policyType)){
	$data = userInfo(@@USER_LOGGED);
	$caseOwner = userInfo(@@CaseOwnerID);
	if((@@underwriterlspsApproval == "1" || @@underwriterlspsApproval == "3" || @@underwriterlspsApproval == "") && (@@motmadstApproval == "1" || @@motmadstApproval == "3" || @@motmadstApproval == "") && (@@propliabApproval == "1" || @@propliabApproval == "3" || @@propliabApproval == "") && (@@enrgaviApproval == "1" || @@enrgaviApproval == "3" || @@enrgaviApproval == "")){
		@@underwriter_reviewed = "1";
		
		$groupName1 = 'PRR_Head_Technical';  //change to name of group

		$result1 = '';

		$to = '';

	
		$query1 = "SELECT DISTINCT GU.USR_UID FROM CONTENT AS C, GROUP_USER AS GU WHERE C.CON_CATEGORY='GRP_TITLE' AND C.CON_VALUE='$groupName1' AND C.CON_ID=GU.GRP_UID";
		$result1 = executeQuery($query1);

		foreach ($result1 as $record) {
		  $aInfo = userInfo($record['USR_UID']);
		  if (empty($to))
			 $to = $aInfo['mail'];
		  else
			 $to .= ', ' . $aInfo['mail'];
	   }
	

	

		if ($to == '') {
		   G::SendMessageText("Unable to find any users in group '$groupName1'.", "WARNING");
		}
		else {
			@@email_salute = $caseOwner['firstname'];
			@@approving_level = "the Head of Technical";
			PMFSendMessage(@@APPLICATION, $data['mail'], $caseOwner['mail'], '', '',
			  'APPROVAL PROCESSMAKER Case ID:'. @@APPNO, 'NotifyNextUser.html');
				@@email_salute = "All";
				PMFSendMessage(@@APPLICATION, $data['mail'], $to, '', '',  'APPROVAL PROCESSMAKER Case ID:'. @@APPNO, 'RouteFeedBack.html');
		}
	}else{
		@@underwriter_reviewed = "0";
		@@email_salute = $caseOwner['firstname'];
		PMFSendMessage(@@APPLICATION, $data['mail'], $caseOwner['mail'], '', '',
		  'APPROVAL PROCESSMAKER Case ID:'. @@APPNO, 'requestDenied.html');
	}
}]]></tri_webbot>
