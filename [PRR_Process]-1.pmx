        <dyn_content><![CDATA[{"name":"headCommercialApprovalForm","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"3589440915a6744614fd983011996425","name":"headCommercialApprovalForm","description":"","mode":"edit","script":{"type":"js","code":"$(\"#title0000000001\").css(\"text-align\",\"center\");\n\nvar noSel = $(\"#policyType\").getValue();\n//if(noSel.length == 0){\n  $(\"#underwriter_lsps\").hide(); $(\"#lspsDate\").hide(); $(\"#lspsApproved\").hide();  $(\"#lspsRmks\").hide();\n  $(\"#pcmmd\").hide();  $(\"#pcmmcDate\").hide();   $(\"#pcmmcApproved\").hide();   $(\"#pcmmcRmks\").hide();\n  $(\"#pcpropliq\").hide();  $(\"#pcpropliqDate\").hide();   $(\"#propLiabApproved\").hide();  $(\"#propliqRmks\").hide();\n  $(\"#pcenergyav\").hide();  $(\"#pceneryavDate\").hide();  $(\"#energyavApproved\").hide(); $(\"#pcenerygyavRmks\").hide();\n//}\n\n$(\"#headTechnical\").hide();  $(\"#headOfTechnicalDate\").hide();  $(\"#headTechApproved\").hide(); $(\"#headTechnicalRmks\").hide();\n$(\"#headCommercial\").hide();  $(\"#headCommercialDate\").hide();  $(\"#headCommApproved\").hide(); $(\"#headCommercialRmks\").hide();\n\n$(\"#budgetaryControlGH\").hide();  $(\"#budgetaryControlGHDate\").hide();  $(\"#budgetConApproved\").hide(); $(\"#budgetaryControlGHRmks\").hide();\n\n$(\"#financeHead\").hide();  $(\"#financeHeadDate\").hide();  $(\"#finApproved\").hide(); $(\"#financeHeadRmks\").hide();\n\n$(\"#chiefRiskOff\").hide();  $(\"#chiefRiskOffDate\").hide();  $(\"#chiefRiskApproved\").hide(); $(\"#chiefRiskOffRmks\").hide();\n\n$(\"#cco\").hide();  $(\"#ccoDate\").hide();  $(\"#ccoApproved\").hide(); $(\"#ccoRmks\").hide();\n\n$(\"#legal_muic\").hide();  $(\"#legalApprovalDate\").hide();  $(\"#legalApproved\").hide(); $(\"#legalRemarks\").hide();\n$(\"#referralApprovingOfficer\").hide();  $(\"#referralOfficerDate\").hide();  $(\"#referralApprovingOfficerApproved\").hide(); $(\"#referralApprovingOfficerRmks\").hide();\n$(\"#insuranceRiskOfficer\").hide();  $(\"#insuranceRiskDate\").hide();  $(\"#insuranceRiskApproved\").hide(); $(\"#insuranceRiskRmks\").hide();\n\nif(noSel.length > 0 ){\n    for(var i = 0; i < noSel.length; i ++ ){\n      \n      if(noSel[i] == \"1\" && $(\"#underwriter_lsps\").getValue() != \"\"){\n      \t $(\"#underwriter_lsps\").show(); $(\"#lspsDate\").show(); $(\"#lspsApproved\").show();  $(\"#lspsRmks\").show();\n      }\n      \n      if(noSel[i] == \"2\" && $(\"#pcmmd\").getValue() != \"\"){\n      \t $(\"#pcmmd\").show();  $(\"#pcmmcDate\").show();   $(\"#pcmmcApproved\").show();   $(\"#pcmmcRmks\").show();\n      }\n      \n      \n      if(noSel[i] == \"3\" && $(\"#pcpropliq\").getValue() != \"\"){\n      \t $(\"#pcpropliq\").show();  $(\"#pcpropliqDate\").show();   $(\"#propLiabApproved\").show();  $(\"#propliqRmks\").show();\n      }\n      \n      if(noSel[i] == \"4\" && $(\"#pcenergyav\").getValue() != \"\"){\n      \t $(\"#pcenergyav\").show();  $(\"#pceneryavDate\").show();  $(\"#energyavApproved\").show(); $(\"#pcenerygyavRmks\").show();\n      }\n      \n    }\n  }\n  \n  if($(\"#headTechnical\").getValue() != \"\"){\n    $(\"#headTechnical\").show();  $(\"#headOfTechnicalDate\").show();  $(\"#headTechApproved\").show(); $(\"#headTechnicalRmks\").show();\n  }\n  \n  if($(\"#headCommercial\").getValue() != \"\"){\n    $(\"#headCommercial\").show();  $(\"#headCommercialDate\").show();  $(\"#headCommApproved\").show(); $(\"#headCommercialRmks\").show();\n  }\n  \n  if($(\"#budgetaryControlGH\").getValue() != \"\"){\n    $(\"#budgetaryControlGH\").show();  $(\"#budgetaryControlGHDate\").show();  $(\"#budgetConApproved\").show(); $(\"#budgetaryControlGHRmks\").show();\n  }\n  \n   if($(\"#financeHead\").getValue() != \"\"){\n    $(\"#financeHead\").show();  $(\"#financeHeadDate\").show();  $(\"#finApproved\").show(); $(\"#financeHeadRmks\").show();\n  }\n  \n  if($(\"#chiefRiskOff\").getValue() != \"\"){\n    $(\"#chiefRiskOff\").show();  $(\"#chiefRiskOffDate\").show();  $(\"#chiefRiskApproved\").show(); $(\"#chiefRiskOffRmks\").show();\n  }\n  \n  if($(\"#cco\").getValue() != \"\"){\n    $(\"#cco\").show();  $(\"#ccoDate\").show();  $(\"#ccoApproved\").show(); $(\"#ccoRmks\").show();\n  }\nif($(\"#legal_muic\").getValue() != \"\" ){\n\t$(\"#legal_muic\").show();  $(\"#legalApprovalDate\").show();  $(\"#legalApproved\").show(); $(\"#legalRemarks\").show();\n}\n\n\nif($(\"#referralApprovingOfficer\").getValue() != \"\" ){\n\t$(\"#referralApprovingOfficer\").show();  $(\"#referralOfficerDate\").show();  $(\"#referralApprovingOfficerApproved\").show(); $(\"#referralApprovingOfficerRmks\").show();\n}\n\n\nif($(\"#insuranceRiskOfficer\").getValue() != \"\") {\n  \t$(\"#insuranceRiskOfficer\").show();  $(\"#insuranceRiskDate\").show();  $(\"#insuranceRiskApproved\").show(); $(\"#insuranceRiskRmks\").show();\n  }\n\n\n$(\"#memoAttached\").disableValidation();\n\n$(\"#powerPointPresentation\").disableValidation();\n$(\"#esmsAttachment\").disableValidation();\n\n\n\nif($(\"#memoAttached\").getValue().length == 0){\n\t$(\"#memoAttached\").hide();\n  $(\"#esmsAttachment\").hide();\n  $(\"#powerPointPresentation\").hide();\n}else{\n  var memoAttached = $(\"#memoAttached\").getValue();\n  \n  $('#memoAttached').show();\n  for(var i = 0; i < memoAttached.length; i ++){\n    if(memoAttached[i] == \"1\"){\n    \t\n  \t\t$('#esmsAttachment').show();\n    }\n    if(memoAttached[i] == \"2\"){\n    \t\n  \t\t$('#powerPointPresentation').show();\n    }\n  }\n    \n}"},"language":"en","externalLibs":"jquery.maskedinput.min.js","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"PPR PROCESS FORM\n","colSpan":12}],[{"colSpan":2},{"type":"text","variable":"customerName","var_uid":"3118054275a674461557cc0051264420","dataType":"string","protectedValue":false,"id":"customerName","name":"customerName","label":"Customer Name","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","valid