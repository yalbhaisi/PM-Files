        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[if(@@lineMgrApproval == "3" || @@underwriterlspsApproval == "3" || @@motmadstApproval == "3" || @@propliabApproval == "3" || @@enrgaviApproval == "3" || @@headTechApprovalAction == "3" || @@headCommercialApprovalAction == "3" || @@budgetaryConGHApprovalAction == "3" || @@headFinancialApprovalAction == "3" || @@chiefRiskOffApprovalAction == "3" || @@ccoApprovalAction == "3"){
	@@isReferred = "1";
}else{
	@@isReferred = "0";
}

if(str_replace(",","",@@totalPremium) >= 5000000.00){
	@@isTresholdReached = "1" ;
}else{
	@@isTresholdReached = "0" ;
}

if(@@referralRequired == ""){
