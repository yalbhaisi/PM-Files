        <dyn_content><![CDATA[{"name":"fillRequestForm","description":"This form will be used to create the request.","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"5322699955a674461505342075350403","name":"fillRequestForm","description":"This form will be used to create the request.","mode":"edit","script":{"type":"js","code":"\nfunction addStylesToFormLabels(controlId){ \n  $(\"#\"+controlId).find(\"label\").css({\"font-size\": \"12px\",\"font-stretch\": \"expanded\",\"line-height\": \"20px\",\"text-align\": \"left\"}); \n}\naddStylesToFormLabels('customerName');\naddStylesToFormLabels('policyNumber');\naddStylesToFormLabels('currency');\naddStylesToFormLabels('totalPremium');\naddStylesToFormLabels('policyType');\naddStylesToFormLabels('requestOfficerRmks');\naddStylesToFormLabels('sumAssured');\naddStylesToFormLabels('esmsAttachment');\naddStylesToFormLabels('powerPointPresentation');\naddStylesToFormLabels('referralRequired');\naddStylesToFormLabels('pprTemplate');\naddStylesToFormLabels('prohibitedActivity');\naddStylesToFormLabels('customerType');\n\n\n$(\"#title0000000001\").css(\"text-align\",\"center\");\n$(\"#label0000000001\").css(\"font-weight\",\"bold\");\n//$(\"#sumAssured\").getControl().maskNumber();\n//$(\"#totalPremium\").getControl().maskNumber();\n\n$(\"#memoAttached\").disableValidation();\n\n$(\"#powerPointPresentation\").disableValidation();\n$(\"#esmsAttachment\").disableValidation();\n\nvar noSel = $(\"#policyType\").getValue();\n\n/*if(noSel.length == 0){\n  $(\"#underwriter_lsps\").hide(); $(\"#lspsDate\").hide(); $(\"#lspsApproved\").hide();  $(\"#lspsRmks\").hide();\n  $(\"#pcmmd\").hide();  $(\"#pcmmcDate\").hide();   $(\"#pcmmcApproved\").hide();   $(\"#pcmmcRmks\").hide();\n  $(\"#pcpropliq\").hide();  $(\"#pcpropliqDate\").hide();   $(\"#propLiabApproved\").hide();  $(\"#propliqRmks\").hide();\n  $(\"#pcenergyav\").hide();  $(\"#pceneryavDate\").hide();  $(\"#energyavApproved\").hide(); $(\"#pcenerygyavRmks\").hide();\n}*/\nif($(\"#customerType\").getValue() == \"\"){\n  $(\"#esmsAttachment\").hide();\n  $(\"#powerPointPresentation\").hide();\n  $(\"#memoAttached\").hide();\n}else if($(\"#customerType\").getValue() == \"1\"){\n\t$(\"#memoAttached\").hide();\n}\n\nif($(\"#lineMgr\").getValue() == \"\" ){\n\t$(\"#9002944115a958d44422ab9094381161\").hide();\n}else {\n  if(noSel.length > 0 ){\n    for(var i = 0; i < noSel.length; i ++ ){\n      \n      if(noSel[i] == \"1\" && $(\"#underwriter_lsps\").getValue() != \"\"){\n      \t $(\"#underwriter_lsps\").show(); $(\"#lspsDate\").show(); $(\"#lspsApproved\").show();  $(\"#lspsRmks\").show();\n      }\n      \n      if(noSel[i] == \"2\" && $(\"#pcmmd\").getValue() != \"\"){\n      \t $(\"#pcmmd\").show();  $(\"#pcmmcDate\").show();   $(\"#pcmmcApproved\").show();   $(\"#pcmmcRmks\").show();\n      }\n      \n      \n      if(noSel[i] == \"3\" && $(\"#pcpropliq\").getValue() != \"\"){\n      \t $(\"#pcpropliq\").show();  $(\"#pcpropliqDate\").show();   $(\"#propLiabApproved\").show();  $(\"#propliqRmks\").show();\n      }\n      \n      if(noSel[i] == \"4\" && $(\"#pcenergyav\").getValue() != \"\"){\n      \t $(\"#pcenergyav\").show();  $(\"#pceneryavDate\").show();  $(\"#energyavApproved\").show(); $(\"#pcenerygyavRmks\").show();\n      }\n      \n    }\n  }\n  \n  if($(\"#headTechnical\").getValue() != \"\"){\n    $(\"#headTechnical\").show();  $(\"#headOfTechnicalDate\").show();  $(\"#headTechApproved\").show(); $(\"#headTechnicalRmks\").show();\n  }\n  \n  if($(\"#headCommercial\").getValue() != \"\"){\n    $(\"#headCommercial\").show();  $(\"#headCommercialDate\").show();  $(\"#headCommApproved\").show(); $(\"#headCommercialRmks\").show();\n  }\n  \n  if($(\"#budgetaryControlGH\").getValue() != \"\"){\n    $(\"#budgetaryControlGH\").show();  $(\"#budgetaryControlGHDate\").show();  $(\"#budgetConApproved\").show(); $(\"#budgetaryControlGHRmks\").show();\n  }\n  \n   if($(\"#financeHead\").getValue() != \"\"){\n    $(\"#financeHead\").show();  $(\"#financeHeadDate\").show();  $(\"#finApproved\").show(); $(\"#financeHeadRmks\").show();\n  }\n  \n  if($(\"#chiefRiskOff\").getValue() != \"\"){\n    $(\"#chiefRiskOff\").show();  $(\"#chiefRiskOffDate\").show();  $(\"#chiefRiskApproved\").show(); $(\"#chiefRiskOffRmks\").show();\n  }\n  \n  \n  if($(\"#cco\").getValue() != \"\"){\n    $(\"#cco\").show();  $(\"#ccoDate\").show();  $(\"#ccoApproved\").show(); $(\"#ccoRmks\").show();\n  }\n  \n  if($(\"#insuranceRiskOfficer\").getValue() != \"\") {\n  \t$(\"#insuranceRiskOfficer\").show();  $(\"#insuranceRiskDate\").show();  $(\"#insuranceRiskApproved\").show(); $(\"#insuranceRiskRmks\").show();\n  }\n  \n}\nfunction customerTypeChangeOperation(){\n  if($(\"#customerType\").getValue() == \"2\"){\n    //$(\"#memoAttached\").prop(\"required\",true);\n    \n    $(\"#memoAttached\").enableValidation()\n  \t$(\"#memoAttached\").show();\n  }else{\n    $(\"#memoAttached\").disableValidation();\n  \t$(\"#memoAttached\").hide();\n    \n    $(\"#powerPointPresentation\").disableValidation();\n    $('#powerPointPresentation').hide();\n    \n    $(\"#esmsAttachment\").disableValidation();\n    $('#esmsAttachment').hide();\n    \n    \n  }\n\t\n}\n$(\"#customerType\").setOnchange(customerTypeChangeOperation);\n/*\nfunction evaluateTotalPremiumAmount(){\n\tvar totalPrem = $(\"#totalPremium\").getValue().replace(/,/g,'');\n  if(totalPrem != \"\"){\n    var totp = parseFloat(totalPrem);\n    if(totp > 0.00 && totp < 5000000.00){\n    \talert(\"total premium is below 5m\");\n    }\n  }\n  \t\n}\n$(\"#totalPremium\").setOnchange(evaluateTotalPremiumAmount);\n\n$(\"#memoAttached\").click(function() {\n  alert('yea you clicked me');\n});*/\n\nfunction showSomeAttachments () {\n var arrCheck = document.getElementsByName('form[memoAttached][]');\n  if(arrCheck[0].checked){\n    $(\"#esmsAttachment\").enableValidation();\n  \t$('#esmsAttachment').show();\n  }else{\n    $(\"#esmsAttachment\").disableValidation();\n  \t$('#esmsAttachment').hide();\n  }\n  \n  if(arrCheck[1].checked){\n    $(\"#powerPointPresentation\").enableValidation();\n  \t$('#powerPointPresentation').show();\n  }else{\n    $(\"#powerPointPresentation\").disableValidation();\n  \t$('#powerPointPresentation').hide();\n  }\n  \n}\nshowSomeAttachments ();\n//leimnud.event.add(document.getElementsByName('form[SimulatorTypeCheck][]'), \"change\", checkSIMExternalTower );\ndocument.getElementsByName('form[memoAttached][]')[0].onchange = showSomeAttachments;\ndocument.getElementsByName('form[memoAttached][]')[1].onchange = showSomeAttachments;"},"language":"en","externalLibs":"jquery.maskedinput.min.js","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"PPR PROCESS FORM\n","colSpan":12}],[{"colSpan":1},{"type":"text","variable":"customerName","var_uid":"3118054275a674461557cc0051264420","dataType":"string","protectedValue":false,"id":"customerName","name":"customerName","label":"Customer Name","defaultValue":"","placeholder":"","hint":"","required":true,"textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"parent","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"customerName","colSpan":5},{"type":"text","variable":"policyNumber","var_uid":"6091425145a674461575ef3064721952","dataType":"string","protectedValue":false,"id":"policyNumber","name":"policyNumber","label":"Policy Number","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"parent","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"policyNumber","colSpan":5},{"colSpan":1}],[{"colSpan":1},{"type":"dropdown","variable":"currency","var_uid":"4658026915b04f51a5196d6099812710","dataType":"string","protectedValue":false,"id":"currency","name":"currency","label":"Currency","defaultValue":"","placeholder