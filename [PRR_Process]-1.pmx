        <tri_uid>9999657225a7dd56aaa3487020269249</tri_uid>
        <tri_title>notifyCCO</tri_title>
        <tri_description></tri_description>
        <pro_uid>6702262155a6744606ca5d1007883498</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[if(@@chiefRiskOffRmks == "" ){
	@@chiefRiskOffRmks = @@textareaVar010;
}else{
	@@chiefRiskOffRmks = @@textareaVar010 . "\n..............\n" . @@chiefRiskOffRmks;
}
$caseOwner = userInfo(@@CaseOwnerID);
if(@@chiefRiskOffApprovalAction == "1" || @@chiefRiskOffApprovalAction == "3"){
	if(@@chiefRiskOffApprovalAction == "1"){
		@@chiefRiskApproved = "Yes";
	}else{
		@@chiefRiskApproved = "Referred";
	}
	
	$data = userInfo(@@USER_LOGGED);
	$groupName1 = 'PRR_CCO_GroupUID';  //change to name of group
	
	$result1 = '';
	
	$to = '';

	
		$query1 = "SELECT DISTINCT GU.USR_UID FROM CONTENT AS C, GROUP_USER AS GU WHERE C.CON_CATEGORY='GRP_TITLE' AND C.CON_VALUE='$groupName1' AND C.CON_ID=GU.GRP_UID";
		$result1 = executeQuery($query1);

		foreach ($result1 as $record) {
		  $aInfo = userInfo($record['USR_UID']);
		  if (empty($to))
			 $to = $aInfo['mail'];
		  else
			 $to .= ', ' . $aInfo['mail'];
	   }
	

	

	if ($to == '') {
	   G::SendMessageText("Unable to find any users in group '$groupName1'.", "WARNING");
	}
	else {
		@@email_salute = $caseOwner['firstname'];
		@@approving_level = "the CCO";
		PMFSendMessage(@@APPLICATION, $data['mail'], $caseOwner['mail'], '', '',
		  'APPROVAL PROCESSMAKER Case ID:'. @@APPNO, 'NotifyNextUser.html');
		
		@@email_salute = 'All';
	   PMFSendMessage(@@APPLICATION, $data['mail'], $to, '', '',  'APPROVAL PROCESSMAKER Case ID:'. @@APPNO, 'RouteFeedBack.html');
	}
}else if(@@chiefRiskOffApprovalAction == "2"){
	//send an email to the requester that their request has been denied
	@@chiefRiskApproved = "No";
	@@approved_by = @@chiefRiskOff;
	$lm = userInfo(@@USER_LOGGED);
	//requestDenied.html
	@@email_salute = $caseOwner['firstname'];
	PMFSendMessage(@@APPLICATION, $lm['mail'], $caseOwner['mail'], '', '',
		  'APPROVAL PROCESSMAKER Case ID:'. @@APPNO, 'requestDenied.html');
}
	]]></tri_webbot>
